{
  "expo": {
    "name": "Instagram",
    "slug": "instagram",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.oshimayoan.instagram"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "dev.oshimayoan.instagram"
    }
  }
}
